{
  "name": "ytsub",
  "version": "1.0.0",
  "description": "YouTube subscription manager TUI",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ytsub": "./dist/cli.js"
  },
  "scripts": {
    "build": "esbuild src/cli.jsx --bundle --platform=node --packages=external --format=esm --outdir=dist --jsx=automatic --out-extension:.js=.js --banner:js='#!/usr/bin/env node' && chmod +x dist/cli.js",
    "start": "pnpm build && node dist/cli.js",
    "dev": "pnpm build && node dist/cli.js",
    "prepublishOnly": "pnpm build",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "youtube",
    "yt-dlp",
    "tui",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ink-tools/ink-mouse": "^2.1.0",
    "execa": "^9.6.1",
    "ink": "^6.6.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "meow": "^13.2.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "esbuild": "^0.27.2"
  }
}
